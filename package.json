{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.15.9",
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer",
    "gen:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package d9-network/interfaces --input ./src/interfaces --endpoint wss://mainnet.d9network.com:40300",
    "gen:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package d9-network/interfaces --endpoint wss://mainnet.d9network.com:40300 --output ./src/interfaces",
    "gen": "npm run gen:defs && npm run gen:meta"
  },
  "dependencies": {
    "@apollo/client": "^3.11.10",
    "@polkadot/api": "^10.13.1",
    "@polkadot/api-contract": "^10.13.1",
    "@polkadot/util": "^12.6.2",
    "@polkadot/util-crypto": "^12.6.2",
    "@unhead/vue": "^1.11.13",
    "@unocss/reset": "^0.58.9",
    "@vue/apollo-composable": "^4.2.1",
    "@vueuse/core": "^10.11.1",
    "@vueuse/head": "^2.0.0",
    "animated-unocss": "^0.0.6",
    "bignumber.js": "^9.1.2",
    "date-fns": "^3.6.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.16.0",
    "gsap": "^3.12.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.8",
    "primevue": "^3.53.0",
    "sass": "^1.82.0",
    "vue": "^3.5.13",
    "vue-demi": "^0.14.10",
    "vue-i18n": "^9.14.2",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@iconify-json/carbon": "^1.2.4",
    "@intlify/unplugin-vue-i18n": "^3.0.1",
    "@polkadot/typegen": "^10.13.1",
    "@polkadot/types": "^10.13.1",
    "@types/bun": "^1.1.14",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.58.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "critters": "^0.0.22",
    "cross-env": "^7.0.3",
    "cypress": "^13.16.0",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.57.1",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-format": "^0.1.3",
    "https-localhost": "^4.7.1",
    "lint-staged": "^15.2.10",
    "pnpm": "^8.15.9",
    "rollup": "^4.28.0",
    "sharp": "^0.33.5",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.9",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "unocss": "^0.58.9",
    "unplugin-auto-import": "^0.17.8",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.8.8",
    "vite": "^5.4.11",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-inspect": "^0.8.9",
    "vite-plugin-vue-devtools": "^7.6.7",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-webfont-dl": "^3.10.2",
    "vite-ssg": "^0.23.8",
    "vite-ssg-sitemap": "^0.6.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.1.10"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
